# .env.example

# Database connection settings
DB_HOST=your_database_host
DB_PORT=your_database_port
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_NAME=your_database_name

# JWT Secret
JWT_SECRET=your_jwt_secret

# API Keys
API_KEY_SERVICE_1=your_api_key_service_1
API_KEY_SERVICE_2=your_api_key_service_2

# AI Service Endpoints
AI_SERVICE_ENDPOINT_1=https://your-ai-service-1-endpoint
AI_SERVICE_ENDPOINT_2=https://your-ai-service-2-endpoint